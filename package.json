    "react-dom": "^16.8.6",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-sound": "^1.2.0",
    "reactotron-react-js": "^2.1.4",
